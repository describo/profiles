{
    "metadata": {
        "name": "Schema profile",
        "description": "Describo profile for creating Schema.org style vocabularies",
        "version": "v0.9"
    },
    "context": [
        "https://w3id.org/ro/crate/1.1/context",
        "https://describo.github.io/type-definitions/context/1.0/context.jsonld",
        {
            "pcdm": "https://pcdm.org/models#",
            "rdf": "https://www.w3.org/1999/02/22-rdf-syntax-ns#",
            "rdfs": "https://www.w3.org/2000/01/rdf-schema#",
            "schema": "https://schema.org",
            "bio": "https://bioschemas.org"
        }
    ],
    "layouts": [
        {
            "appliesTo": ["Dataset"],
            "about": {
                "label": "About",
                "description": "",
                "order": 0,
                "properties": ["license", "description", "author", "contributor", "publisher"]
            },
            "definitions": {
                "label": "Definitions",
                "description": "",
                "order": 1,
                "properties": ["mentions"]
            },
            "overflow": {
                "label": "Other",
                "order": 2
            }
        }
    ],
    "classes": {
        "Dataset": {
            "definition": "override",
            "subClassOf": [],
            "inputs": [
                {
                    "id": "http://schema.org/license",
                    "name": "license",
                    "help": "A license document that applies to this content, typically indicated by URL.",
                    "type": ["URL", "CreativeWork"],
                    "multiple": false
                },
                {
                    "id": "http://schema.org/description",
                    "name": "description",
                    "help": "An abstract of the collection. Include as much detail as possible about the motivation and use of the collection, including things that we do not yet have properties for",
                    "type": ["TextArea"],
                    "multiple": false
                },
                {
                    "id": "http://schema.org/author",
                    "name": "author",
                    "help": "The person or organization responsible for creating this collection of data",
                    "type": ["Person", "Organization"],
                    "multiple": true
                },
                {
                    "id": "http://schema.org/contributor",
                    "name": "contributor",
                    "help": "A secondary contributor to the CreativeWork or Event.",
                    "type": ["Person", "Organization"],
                    "multiple": true
                },
                {
                    "id": "http://schema.org/publisher",
                    "name": "publisher",
                    "help": "The organization responsible for releasing this collection of data",
                    "type": ["Organization"],
                    "multiple": true
                },

                {
                    "id": "http://schema.org/mentions",
                    "name": "mentions",
                    "help": "The Classes, Properties etc",
                    "type": ["rdfs:Class", "rdf:Property", "definedTerm"],
                    "multiple": true
                },
                {
                    "id": "https://schema.org/mainEntity",
                    "name": "mainEntity",
                    "help": "The main Thing that this dataset is about.",
                    "multiple": true,
                    "type": ["ANY", "Thing", "URL"],
                    "group": "about"
                }
            ]
        },
        "rdfs:Class": {
            "definition": "override",
            "subClassOf": [],
            "inputs": [
                {
                    "id": "http://www.w3.org/2000/01/rdf-schema#label",
                    "name": "rdfs:label",
                    "label": "label",
                    "help": "The label for this class",
                    "required": true,
                    "multiple": false,
                    "type": ["Text"]
                },
                {
                    "id": "http://www.w3.org/2000/01/rdf-schema#comment",
                    "name": "rdfs:comment",
                    "label": "comment",
                    "help": "A description of the Class",
                    "required": true,
                    "multiple": false,
                    "type": ["TextArea"]
                },
                {
                    "id": "http://www.w3.org/2000/01/rdf-schema#subClassOf",
                    "name": "rdfs:subClassOf",
                    "label": "subClassOf",
                    "help": "Parent Class (if any)",
                    "required": true,
                    "multiple": false,
                    "type": ["rdfs:Class"]
                },
                {
                    "id": "https://schema.org/sameAs",
                    "name": "schema:sameAs",
                    "help": "URL of an equivalent term",
                    "multiple": false,
                    "type": ["URL"]
                }
            ]
        },
        "rdf:Property": {
            "definition": "override",
            "subClassOf": [],
            "inputs": [
                {
                    "id": "http://www.w3.org/2000/01/rdf-schema#label",
                    "name": "rdfs:label",
                    "label": "label",
                    "help": "The label for this property",
                    "required": true,
                    "multiple": false,
                    "type": ["Text"]
                },
                {
                    "id": "http://www.w3.org/2000/01/rdf-schema#comment",
                    "name": "rdfs:comment",
                    "label": "comment",
                    "help": "A description of the Property",
                    "required": true,
                    "multiple": false,
                    "type": ["TextArea"]
                },
                {
                    "id": "http://www.w3.org/2000/01/rdf-schema#subPropertyOf",
                    "name": "rdfs:subPropertyOf",
                    "label": "subPropertyOf",
                    "help": "Is this a sub-type of a more general property",
                    "multiple": false,
                    "type": ["rdfs:Class"]
                },
                {
                    "id": "http://schema.org/domainIncludes",
                    "name": "schema:domainIncludes",
                    "label": "domainIncludes",
                    "help": "What classes use this property (ie what domain does this property occur in)",
                    "required": true,
                    "multiple": true,
                    "type": ["rdfs:Class"]
                },
                {
                    "id": "http://schema.org/rangeIncludes",
                    "name": "schema:rangeIncludes",
                    "label": "rangeIncludes",
                    "help": "What classes are expected values for this range",
                    "required": true,
                    "multiple": true,
                    "type": ["rdfs:Class", "DefinedTerm"]
                },
                {
                    "id": "https://schema.org/sameAs",
                    "name": "schema:sameAs",
                    "help": "URL of an equivalent term",
                    "type": ["URL"],
                    "multiple": false
                }
            ]
        },
        "DefinedTerm": {
            "definition": "override",
            "subClassOf": [],
            "inputs": [
                {
                    "id": "https://schema.org/description",
                    "name": "schema:description",
                    "help": "A description of the defined Term",
                    "type": ["TextArea"],
                    "multiple": false
                },
                {
                    "id": "https://schema.org/sameAs",
                    "name": "schema:sameAs",
                    "help": "URL of an equivalent term",
                    "type": ["URL"],
                    "multiple": false
                }
            ]
        }
    }
}
